{
  "name": "markdown-preview-bitbucket",
  "displayName": "Markdown Preview with Bitbucket Styles",
  "description": "Replaces the Markdown preview with Bitbucket styles.",
  "version": "1.0.0",
  "publisher": "hbrok",
  "license": "MIT",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#eee",
    "theme": "light"
  },
  "keywords": [
    "Markdown",
    "Bitbucket"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./extension",
  "contributes": {
    "markdown.previewStyles": [
      "./base.css",
      "./bitbucket-markdown.css"
    ],
    "markdown.markdownItPlugins": true
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "eslint": "^5.13.0",
    "vscode": "^1.1.29"
  },
  "dependencies": {
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-table-of-contents": "^0.4.3"
  },
  "bugs": {
    "url": "https://github.com/hbrok/markdown-preview-bitbucket/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hbrok/markdown-preview-bitbucket.git",
    "homepage": "https://github.com/hbrok/markdown-preview-bitbucket"
  }
}
